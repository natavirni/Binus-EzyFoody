        <activity
            android:name=".Topup"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Foods"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Snacks"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MyOrder"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Order"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Drinks"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Restaurant"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>